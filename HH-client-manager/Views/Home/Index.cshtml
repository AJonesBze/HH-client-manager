@{
    ViewData["Title"] = "Import Demo Page";
}


<p><br/><br/><a asp-controller="Home" asp-action="Export">Export Generic Test File</a></p>

<form method="post" enctype="multipart/form-data" asp-controller="UploadFiles" asp-action="Index">
    <div class="form-group">
        <div class="col-md-10">
            <p>Upload one or more files using this form:</p>
            <input type="file" name="files" accept=".xlsx" multiple />
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-10">
            <input type="submit" value="Upload" />
        </div>
    </div>
</form>


@if (ViewData["FileTables"] != null)
{
<p>
    Here you are, then: <br/> @ViewData["FileTables"]
</p>
}